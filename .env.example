# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Backend Base URL (for CORS and self-reference)
BACKEND_URL=https://potential-club-backend.onrender.com

# Environment
ENVIRONMENT=production

# CORS Origins (comma-separated)
# Add your frontend domain here when deploying
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,https://your-frontend-domain.vercel.app

# Optional: WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30
WS_TIMEOUT=300

# Optional: Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
